{
  "name": "brainstrike-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "lint:fix": "eslint './src/**/*.{ts,tsx}' --fix",
    "generate": "graphql-codegen --config codegen.yml",
    "generate:dev": "graphql-codegen --config codegen.yml --watch",
    "apollo:schema:download": "apollo schema:download --endpoint=http://localhost:4000/graphql graphql.schema.json",
    "dev": "concurrently \"yarn run start\" \"yarn run generate:dev\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "@graphql-codegen/**/mem": "^6.0.1"
  },
  "dependencies": {
    "@apollo/react-hooks": "3.1.3",
    "@material-ui/core": "^4.9.1",
    "@material-ui/icons": "^4.9.1",
    "@types/jest": "25.1.1",
    "@types/node": "13.7.0",
    "@types/react": "16.9.19",
    "@types/react-dom": "16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/yup": "^0.26.29",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-http": "^1.5.16",
    "apollo-utilities": "^1.3.3",
    "concurrently": "^5.1.0",
    "formik": "^2.1.3",
    "formik-material-ui": "^1.0.1",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.1",
    "typescript": "3.7.5",
    "yup": "^0.28.1"
  },
  "devDependencies": {
    "@apollo/react-testing": "3.1.3",
    "@graphql-codegen/cli": "^1.12.1",
    "@graphql-codegen/introspection": "1.12.1",
    "@graphql-codegen/typescript": "^1.12.1",
    "@graphql-codegen/typescript-operations": "1.12.1",
    "@graphql-codegen/typescript-react-apollo": "1.12.1",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.0",
    "@typescript-eslint/eslint-plugin": "^2.19.0",
    "@typescript-eslint/parser": "^2.19.0",
    "apollo": "^2.22.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "prettier": "1.19.1",
    "rimraf": "^3.0.1"
  }
}
