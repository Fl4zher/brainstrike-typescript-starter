{
  "name": "brainstrike-server",
  "version": "1.0.0",
  "description": "brainkstrike typescript node API",
  "main": "build/index.js",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "start:dev": "nodemon",
    "start": "npm run build && node build/index.js",
    "test": "jest --watch",
    "lint:fix": "eslint './src/**/*.{ts,tsx}' --fix",
    "coverage": "jest --coverage",
    "generate": "graphql-codegen --config codegen.yml",
    "generate:dev": "graphql-codegen --config codegen.yml --watch",
    "dev": "concurrently \"npm run start:dev\" \"npm run generate:dev\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seandearnaley/brainstrike-server.git"
  },
  "keywords": [
    "api",
    "node",
    "apollo",
    "typescript"
  ],
  "author": "Sean Dearnaley <seandearnaley@hotmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seandearnaley/brainstrike-server/issues"
  },
  "homepage": "https://github.com/seandearnaley/brainstrike-server#readme",
  "devDependencies": {
    "@graphql-codegen/cli": "^1.8.3",
    "@graphql-codegen/typescript": "1.8.3",
    "@graphql-codegen/typescript-resolvers": "1.8.3",
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.11",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "apollo-link": "^1.2.13",
    "apollo-link-http": "^1.5.16",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "nock": "^11.7.0",
    "node-fetch": "^2.6.0",
    "nodemon": "^1.19.4",
    "prettier": "1.19.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "@types/graphql-iso-date": "^3.3.3",
    "@types/node-fetch": "^2.5.3",
    "apollo-server-express": "^2.9.9",
    "apollo-server-testing": "^2.9.9",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql-import": "^0.7.1",
    "graphql-iso-date": "^3.6.1",
    "graphql-tag": "^2.10.1",
    "lodash": "^4.17.15",
    "mem": ">=6.0.0",
    "pg": "^7.13.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^4.1.0",
    "typeorm": "^0.2.20"
  }
}
